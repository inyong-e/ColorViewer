{
  "manifest_version": 2,

  "name": "what is the main color",
  "description": "웹 페이지에서 사용된 주요 색상을 보여주는 프로그램",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "tabs"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "all_frames": true,
      "js": ["javascript/controller.js"]
    }
  ]
}
